{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
          
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ],
    "routes": [
      {
        "src": "/_next/(.*)",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      }
    ],
    "redirects": [
      {
        "source": "/old-route",
        "destination": "/new-route",
        "permanent": true
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=0, s-maxage=86400, stale-while-revalidate"
          }
        ]
      }
    ],
    "error": {
      "notFound": {
        "destination": "/404",
        "statusCode": 404
      }
    }
  }
  
